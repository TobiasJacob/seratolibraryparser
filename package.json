{
  "name": "seratolibraryparser",
  "version": "1.3.0",
  "description": "Helps parsing serato DJ libraries for node.js based applications.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "docs": "typedoc --mode file --out docs src/index.ts",
    "demo": "ts-node ./src/demo.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint && npm run docs",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Tobias Jacob",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^13.9.4",
    "jest": "^25.2.2",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.8.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.17.0-0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/plist": "^3.0.2",
    "plist": "^3.0.1"
  },
  "keywords": [
    "Serato",
    "DJ",
    "Scratch",
    "Live"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TobiasJacob/seratolibraryparser"
  }
}
